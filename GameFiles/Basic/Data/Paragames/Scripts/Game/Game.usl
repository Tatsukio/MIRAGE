			if(!bStartLeft)then
				xKey.m_xTime=xNow;
				xKey.m_fWeight=0.0f;
				xD.AddKey(xKey);

				xKey.m_xTime=xNow+(CGameTimeSpan.OneSecond()*1.0f);
				xKey.m_fWeight=1.0f;
				xD.AddKey(xKey);

				xKey.m_xTime=xNow+(CGameTimeSpan.OneSecond()*1.2f);
				xKey.m_fWeight=1.0f;
				xD.AddKey(xKey);
			endif;

			xKey.m_xTime=xNow+(CGameTimeSpan.OneSecond()*2.2f);
			xKey.m_fWeight=0.0f;
			xD.AddKey(xKey);

			if(bStartLeft)then
