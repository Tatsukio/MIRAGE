	proc bool CapVid(bool p_bCapture)
		if(p_bCapture)then
			//L CGameWrap.LogInfo("Game","Capture Start");
			m_pxDesktop^.CaptureStart("GameCapture.raw");
		else
			//L CGameWrap.LogInfo("Game","Capture Stop");
			m_pxDesktop^.CaptureStop();
		endif;
		return(!p_bCapture);
	endproc;

