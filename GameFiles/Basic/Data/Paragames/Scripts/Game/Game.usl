		if(CUIStateMgr.Get().IsIngame()) then 
			var ^CInGameScreen pxInGameScreen = CUIStateMgr.Get().GetInGameScreen();
			if(pxInGameScreen!=null)then				
				pxInGameScreen^.GetDlgSceneMgr()^.PushScene(p_sScene);
			endif;
		endif;
	endproc;

