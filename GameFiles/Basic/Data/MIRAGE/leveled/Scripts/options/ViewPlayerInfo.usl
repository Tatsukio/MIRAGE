			m_apxMinValues[i]=cast<CDropList>(GetControl("GroupChars/Min"+(i+1).ToString()));
		endfor;
		for(i=0)cond(i<iC)iter(i++)do
			m_apxMaxValues[i]=cast<CDropList>(GetControl("GroupChars/Max"+(i+1).ToString()));
		endfor;
		for(i=0)cond(i<26)iter(i++)do
