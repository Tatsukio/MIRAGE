	export constructor(^CPropDB.CNode p_pxPlayerNode,^CPropDB.CNode p_pxCharsNode,^CTribeViewLayoutMgr p_pxTribeView)
		if(!InitFromResource("options/ViewPlayerInfo","LimitsChars")) then
