	export static proc bool CanBeControlled(int p_iHitOwner)
		var bool bAllyCommands = CMirageClnMgr.Get().CheckAlienCommands();
		var int iPlayerID = CGameWrap.GetClientID();
		var int iMyOpinion = CGameWrap.GetDiplomacyMgr().GetMyOpinion(p_iHitOwner);
		var int iTheirOpinion = CGameWrap.GetDiplomacyMgr().GetTheirOpinion(p_iHitOwner);

		if (p_iHitOwner == -1)then
			return false;
		elseif (p_iHitOwner == iPlayerID)then
			return true;
		elseif (bAllyCommands == false)then
			return false;
		elseif (iMyOpinion == 2 && iTheirOpinion == 2)then
			return true;
		endif;

		return false;
	endproc;
	
