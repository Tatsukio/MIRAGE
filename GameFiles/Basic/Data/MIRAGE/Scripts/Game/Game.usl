				CMusicMgr.Get().PlayMusic(sMapName);
				CMusicMgr.Get().SetEnabled(true);
			endif;
		endif;
		*/
		//END MUSIC HACK
		var ^CMainController pxMainCtrl=^(CMainController.Get());
