					RotateActionSpeed(qRot,5.0);
					p_rbRotated=true;
					return false;
				endif;
			endif;
		end Rotate;
		if(GetPenetration())then
			Penetrate(p_pxEnemy,p_vTarget);
			if(sFAnim!=""&&HasAnim(sFAnim))then
				SetAnim(sFAnim,1);
			endif;
			return false;
		elseif(bProj)then
			var int j, iK=GetProjectilesNumber();
			for(j=0)cond(j<iK)iter(j++)do
				CreateProjectileAndShoot(p_pxEnemy, p_vTarget);
			endfor;
			if(sFAnim!=""&&HasAnim(sFAnim))then
				SetAnim(sFAnim,1);
			endif;
			DoCaptainAttackAnim();
			return false;
		elseif(bFAnim)then
			if(sFAnim!=""&&HasAnim(sFAnim))then
				SetAnim(sFAnim,1);
			endif;
			DoCaptainAttackAnim();
		endif;
		return true;
	endproc;
	
