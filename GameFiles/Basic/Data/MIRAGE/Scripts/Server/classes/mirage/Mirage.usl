			if(HasLink(xLink))then
				SetLinkGFX(xLink,"spray_back_"+p_iSize.ToString());
			endif;
			xLink="SpFr";
			if(HasLink(xLink))then
				SetLinkGFX(xLink,"spray_front_"+p_iSize.ToString());
