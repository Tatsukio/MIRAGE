			if(!pxDef^.HasFilter(sNormal)&&!pxDef^.HasFilter(sNew))then
				pxDef^.EnableFilter(sNormal);
			endif;
		endif;
	endproc;
	
	export proc void HandleAction(string p_sAction)
		if(p_sAction.Find("aje_catapult_rex_spear")>=0)then
