		var CFourCC xBase="ACTx";
		var ^CUOFWriterNode pxWalk=p_pxWriterNode^.AddSubChunk(xBase,1);
		var ^CArc pxArc=^(pxWalk^.GetArc());
		pxArc^<<m_iBuildUp;
		pxWalk^.Close();
