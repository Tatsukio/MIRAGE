	endproc;
	
	export proc void WoundDamage(real p_fDamage, CObjHndl p_xLE, int p_iLE)
		return;
	endproc;
	
	export proc bool GetNeutralObject()
		return true;
	endproc;
	
	export proc string GetReportCode()
		return "";
	endproc;
	
