				m_iBuildUp=0;
				DestroyAdditionalBuildUps();
				if(BuildSpears())then
					UpdateGfx();
				endif;
			endif;
		elseif(p_sAction.Find("aje_catapult_rex_egg")>=0)then
			if(m_iBuildUp!=2)then
