			CreateTimer(LIFETIME_TIMER, CGameTimeSpan.OneSecond() * LIFETIME, false);
			CreateTimer(CHECK_ALARM_RANGE, CGameTimeSpan.OneSecond() * ALARM_RANGE_CHECK_INTERVAL, true);
			ExamineEnemies(true);
//			OnActionEnd(false);
//			AutoScout();
		endif;
	endproc;
	
