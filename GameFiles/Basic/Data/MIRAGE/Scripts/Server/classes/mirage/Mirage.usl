		if(!IsDead())then
			CreateCorpse();
		endif;
		super.Die();
	endproc;
	
	export proc void CreateCorpse()
		var ^CCargolifterCorpse pxGameObj=cast<CCargolifterCorpse>(CSrvWrap.GetObjMgr()^.CreateObj("dirigible_corpse",GetOwner(),GetPos(),GetRotation()));
