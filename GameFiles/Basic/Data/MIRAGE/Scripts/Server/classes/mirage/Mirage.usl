	proc bool UpdateDestructionFlags()
		if(((GetHitpoints()!=0.0f)&&(GetMaxHitpoints()!=0.0f)))then
			var real fPercentage=GetHitpoints()/(GetMaxHitpoints()/100.0f);
			if(fPercentage<=15.0f)then
				if(GetRndInvMaskSingleFlagInv(VIS_FLAG_CHTR_HEAD))then
					SetRndInvMaskSingleFlagInv(VIS_FLAG_CHTR_HEAD,false);
				endif;
				super.UpdateDestructionFlags();
			elseif(fPercentage<=25.0f)then
				if(!GetRndInvMaskSingleFlagInv(VIS_FLAG_CHTR_HEAD))then
					SetRndInvMaskSingleFlagInv(VIS_FLAG_CHTR_HEAD,true);
				endif;
				super.UpdateDestructionFlags();
				var CFourCC xLink="FIRE";
				var CObjHndl xFire=AddFX("avatar_fire",-1.0);
				if(xFire.IsValid() && !HasGroupedChildren())then
					xFire.GetObj()^.LinkAction(GetHandle(),xLink);
					AddGroupedChildren(xFire.GetObj()^.GetGuid());
				endif;
				if(xFire.GetObj()^.GetDestructLevel()!=GetDestructLevel())then
					xFire.GetObj()^.SetDestructLevel(GetDestructLevel());
				endif;
			elseif(fPercentage<=50.0f)then
				if(!GetRndInvMaskSingleFlagInv(VIS_FLAG_CHTR_HEAD))then
					SetRndInvMaskSingleFlagInv(VIS_FLAG_CHTR_HEAD,true);
				endif;
				super.UpdateDestructionFlags();
				var CFourCC xLink="FIRE";
				var CObjHndl xFire=AddFX("avatar_fire",-1.0);
				if(xFire.IsValid() && !HasGroupedChildren())then
					xFire.GetObj()^.LinkAction(GetHandle(),xLink);
					AddGroupedChildren(xFire.GetObj()^.GetGuid());
				endif;
				if(xFire.GetObj()^.GetDestructLevel()!=GetDestructLevel())then
					xFire.GetObj()^.SetDestructLevel(GetDestructLevel());
				endif;
			else
				super.UpdateDestructionFlags();
				if(!GetRndInvMaskSingleFlagInv(VIS_FLAG_CHTR_HEAD))then
					SetRndInvMaskSingleFlagInv(VIS_FLAG_CHTR_HEAD,true);
				endif;
				if(HasGroupedChildren())then
					RemGroupedChildren((AddFX("avatar_fire",-1.0)).GetObj()^.GetGuid());
					RemoveFX("avatar_fire");
				endif;
			endif;
		endif;
		return true;
	endproc;
	
