		if(m_pxConqueror2!=null)then
			m_pxConqueror2^.ReduceProductionTime();
		endif;
		if(m_pxConqueror3!=null)then
			m_pxConqueror3^.ReduceProductionTime();
		endif;
		if(m_pxConqueror4!=null)then
			m_pxConqueror4^.ReduceProductionTime();
		endif;
	endproc;
	
