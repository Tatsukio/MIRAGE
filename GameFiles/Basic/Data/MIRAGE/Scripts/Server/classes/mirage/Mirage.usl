		if((GetOwner()==pxFO^.GetOwner()) || (AuraSharing() && CSrvWrap.GetDiplomacyMgr().GetIsFriend(pxFO^.GetOwner(), GetOwner())&&CSrvWrap.GetDiplomacyMgr().GetIsFriend(GetOwner(), pxFO^.GetOwner())))then
