			var int iVersion=p_pxReaderNode^.GetVersion();
			if(iVersion>=1)then
				(pxArc^) << m_bEquipped;
				var int i,iC;
				(pxArc^) << iC;
				m_avAddonOffsets=iC;
				for(i=0) cond(i<iC) iter(i++)do
					(pxArc^) << m_avAddonOffsets[i];
				endfor;
				m_xAddons.DoKArc(pxArc^);
			endif;
		else
			super.Load(p_pxReaderNode);
		endif;
	endproc;
	
	export proc void Save(^CUOFWriterNode p_pxWriterNode)
		super.Save(p_pxWriterNode);
		var CFourCC xBase="SeLa";
		var ^CUOFWriterNode pxWalk=p_pxWriterNode^.AddSubChunk(xBase,1);
		var ^CArc pxArc=^(pxWalk^.GetArc());
		(pxArc^) << m_bEquipped;
		var int i,iC=m_avAddonOffsets.NumEntries();
		(pxArc^) << iC;
		for(i=0) cond(i<iC) iter(i++)do
			(pxArc^) << m_avAddonOffsets[i];
		endfor;
		m_xAddons.DoKArc(pxArc^);
		pxWalk^.Close();
	endproc;
	
	export proc void SetReady()
		super.SetReady();
		if(m_bBuildingReady)then
			BuildAddons();
			HideSmoke(true);
		endif;
	endproc;
	
	proc void BuildAddons()
		if(!m_bEquipped)then
			m_bEquipped=true;
			var CFourCC xLink="NOPE";
			var ^CMirageDeco pxA;
			m_avAddonOffsets.AddEntry({-3.4,-7.0,11.51}); //chimney
			m_avAddonOffsets.AddEntry({-9.2,-8.4,-0.4}); //crane
			var int i, iC=m_avAddonOffsets.NumEntries();
			for(i=0)cond(i<iC)iter(i++)do
				pxA=cast<CMirageDeco>(CSrvWrap.GetObjMgr()^.CreateObj("seas_laboratory_misc_"+i.ToString(),GetOwner(),GetPos(),GetRotation()));
				if(pxA!=null)then
					m_xAddons.AddEntry(pxA^.GetHandle());
					AddGroupedChildren(pxA^.GetGuid());
					var vec3 vDir=pxA^.GetPos()-GetPos();
					var Quat qRot;
					qRot.FromXYDir(vDir.GetX(),vDir.GetY());
					if(i==0)then
						qRot.RotateZ(-0.4538);
					else
						qRot.RotateZ(1.047);
					endif;
					pxA^.SecRotAction(qRot,0.01);
					pxA^.LinkAction(GetHandle(),xLink,m_avAddonOffsets[i]);
				endif;
			endfor;
		endif;
	endproc;
	
	export proc void OnPostLoad()
		super.OnPostLoad();
		var CFourCC xLink="NOPE";
		var int i, iC=m_xAddons.NumEntries();
		for(i=0)cond(i<iC)iter(i++)do
			var ^CGameObj pxA=m_xAddons[i].GetObj();
			if(pxA!=null)then
				pxA^.LinkAction(GetHandle(),xLink,m_avAddonOffsets[i]);
			endif;
		endfor;
	endproc;
	
	export proc void TransferLinkedObjs(CObjHndl p_xC)
		if(!p_xC.IsValid())then return; endif;
		var CFourCC xLink="NOPE";
		while(m_xAddons.NumEntries()>0)do
			var ^CGameObj pxA = m_xAddons[0].GetObj();
			if(pxA==null)then
				m_xAddons.DeleteEntry(0);
				m_avAddonOffsets.DeleteEntry(0);
				continue;
			endif;
			RemGroupedChildren(pxA^.GetGuid());
			pxA^.TerminateAction();
			pxA^.LinkAction(p_xC,xLink,m_avAddonOffsets[0]);
			m_xAddons.DeleteEntry(0);
			m_avAddonOffsets.DeleteEntry(0);
		endwhile;
		m_xAddons.Clear();
	endproc;
	
	export proc CObjList GetAddons();
		return m_xAddons;
	endproc;

	//////////////////////// WORK FAKE ANIMS ////////////////////////////////
	
	export proc void OnWork()
		DeleteTimer(FAKE_WORK_ANIM_TIMER_ID);
		CreateTimer(FAKE_WORK_ANIM_TIMER_ID,CGameTimeSpan.OneSecond()*FAKE_WORK_ANIM_TIMEOUT,false);
		if(m_xAddons.NumEntries()!=2)then return; endif;
		var ^CGameObj pxCraneObj=m_xAddons[1].GetObj();
		if(pxCraneObj==null)then return; endif;
		if((pxCraneObj^.GetCurrentAnimName()!=FAKE_WORK_ANIM_NAME||pxCraneObj^.GetCurrentAnimMode()!=3)&&pxCraneObj^.HasAnim(FAKE_WORK_ANIM_NAME))then
			pxCraneObj^.SetAnim(FAKE_WORK_ANIM_NAME,3);
			HideSmoke(false);
		endif;
	endproc;
	
	export proc void OnWorkFinished()
		if(m_xAddons.NumEntries()!=2)then return; endif;
		var ^CGameObj pxCraneObj=m_xAddons[1].GetObj();
		if(pxCraneObj==null)then return; endif;
		if(pxCraneObj^.HasAnim(WORK_FINISHED_ANIM_NAME_1))then
			if(pxCraneObj^.HasAction())then
				pxCraneObj^.SetAnim(FAKE_WORK_FINISHED_ANIM_NAME,1);
			else
				pxCraneObj^.AnimAction(FAKE_WORK_FINISHED_ANIM_NAME);
			endif;
			HideSmoke(true);
		else
			SetIdleAnim();
		endif;
	endproc;
	
	export proc void OnWorkFinishedSimple()
		HideSmoke(true);
		SetIdleAnim();
	endproc;
	
	export proc void StartWorkAnim()
		DeleteTimer(FAKE_WORK_ANIM_TIMER_ID);
		CreateTimer(FAKE_WORK_ANIM_TIMER_ID,CGameTimeSpan.OneSecond()*FAKE_WORK_ANIM_TIMEOUT,false);
		if(m_xAddons.NumEntries()!=2)then return; endif;
		var ^CGameObj pxCraneObj=m_xAddons[1].GetObj();
		if(pxCraneObj==null)then return; endif;
		if(pxCraneObj^.GetCurrentAnimName()!=FAKE_WORK_ANIM_NAME&&pxCraneObj^.HasAnim(FAKE_WORK_ANIM_NAME))then
			pxCraneObj^.SetAnim(FAKE_WORK_ANIM_NAME,3);
			HideSmoke(false);
		endif;
	endproc;
	
	export proc void DoWorkAnim()
		if(m_xAddons.NumEntries()!=2)then return; endif;
		var ^CGameObj pxCraneObj=m_xAddons[1].GetObj();
		if(pxCraneObj==null)then return; endif;
		if((pxCraneObj^.GetCurrentAnimName()!=FAKE_WORK_ANIM_NAME||pxCraneObj^.GetCurrentAnimMode()!=3)&&pxCraneObj^.HasAnim(FAKE_WORK_ANIM_NAME))then
			HideSmoke(false);
			pxCraneObj^.SetAnim(FAKE_WORK_ANIM_NAME,3);
		endif;
	endproc;
	
	export proc void StopWorkAnim()
		if(m_xAddons.NumEntries()!=2)then return; endif;
		var ^CGameObj pxCraneObj=m_xAddons[1].GetObj();
		if(pxCraneObj==null)then return; endif;
		if(pxCraneObj^.GetCurrentAnimName()==FAKE_WORK_ANIM_NAME)then
			HideSmoke(true);
			pxCraneObj^.EndCurrentLoopAnim();
		endif;
	endproc;
	
	export proc void HideSmoke(bool p_bIsHidden)
		if(m_xAddons.NumEntries()!=2)then return; endif;
		var ^CGameObj pxSmokeObj=m_xAddons[0].GetObj();
		if(pxSmokeObj==null)then return; endif;
		pxSmokeObj^.SetNoParticles(p_bIsHidden);
	endproc;
	
	export proc void HandleEvent(ref CGameEvtPtr p_rxEvtPtr)
		if(p_rxEvtPtr.GetClass()==ms_xTimerClass)then
			if(p_rxEvtPtr.GetInt(0)==FAKE_WORK_ANIM_TIMER_ID)then
				StopWorkAnim();
			else
				super.HandleEvent(p_rxEvtPtr);
			endif;
		else
			super.HandleEvent(p_rxEvtPtr);
		endif;
	endproc;

	export proc void Delete()
		StopWorkAnim();
		var int i, iC=m_xAddons.NumEntries();
		for(i=0)cond(i<iC)iter(i++)do
			var ^CGameObj pxA=m_xAddons[i].GetObj();
			if(i==1)then continue; endif;
			if(pxA!=null)then
				RemGroupedChildren(pxA^.GetGuid());
				pxA^.Delete();
			endif;
		endfor;
		super.Delete();
	endproc;
	
endclass;
	
