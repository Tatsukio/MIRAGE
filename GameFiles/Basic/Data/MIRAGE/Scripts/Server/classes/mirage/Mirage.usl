	export proc void OnInit(bool p_bLoad)
		super.OnInit(p_bLoad);
		if(!p_bLoad)then
			var CFourCC xLink="Nest";
			var ^CNestBuildUp pxNest=cast<CNestBuildUp>(CSrvWrap.GetObjMgr()^.CreateObj("aje_fake_nest",GetOwner(),GetPos()));
