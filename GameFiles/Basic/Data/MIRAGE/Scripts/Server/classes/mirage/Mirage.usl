		m_bSiegeBreak=p_bOn;
	endproc;
	
	export proc void DiePerHarakiri()
		if(m_xClimbers.Validate()>0)then return; endif;
		return super.DiePerHarakiri();
	endproc;
	
	export proc string GetReportCode()
		return "unit";
	endproc;
	
