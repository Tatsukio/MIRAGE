	export proc void Load(^CUOFReaderNode p_pxNode)
		if(p_pxNode^.GetType()=="PoMi")then
			var int iVersion=p_pxNode^.GetVersion();
			var ^CArc pxArc=^(p_pxNode^.GetArc());
			var int i, iC;
			if(iVersion<4)then
				(pxArc^) << iC;
				for(i=0)cond(i<iC)iter(i++)do
					var ^CPoolMixer.CSpiritInfo pxPack=^(m_axSpiritPack.NewEntryRef());
					pxPack^.DoKArc(pxArc^,iVersion);
				endfor;
			endif;
			if(iVersion>=2)then
				pxArc^<<m_bLeveling;
			endif;
			if(iVersion>=3)then
				pxArc^<<m_bReapingOver;
				pxArc^<<m_iSeer;
				pxArc^<<m_iDefenders;
				pxArc^<<m_iConquerors;
				pxArc^<<m_iConqueror1;
				pxArc^<<m_iConqueror2;
				pxArc^<<m_iConqueror3;
				pxArc^<<m_iConqueror4;
				pxArc^<<m_iPaladins;
				pxArc^<<m_iPaladin1;
				pxArc^<<m_iPaladin2;
				pxArc^<<m_iPaladin3;
				pxArc^<<m_iPaladin4;
				pxArc^<<m_iPhantomIncrease;
				pxArc^<<m_iPaladinIncrease;
				pxArc^<<m_iPhantomGrowth;
				pxArc^<<m_fBounty;
				CMirageSrvMgr.ms_fBounty=m_fBounty;
			endif;
			if(iVersion>=4)then
				(pxArc^) << iC;
				m_asConquerors=iC;
				for(i=0)cond(i<iC)iter(i++)do
					pxArc^<<m_asConquerors[i];
				endfor;
			endif;
		else
			super.Load(p_pxNode);
		endif;
	endproc;
	
