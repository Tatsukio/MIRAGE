	var bool 		m_bActive;
	var bool 		m_bFirstStage;
	var bool 		m_bFinalStage;
	var CObjHndl 	m_xBuildDeco;
	
	export constructor()
		m_bActive=false;
		m_bFirstStage=false;
		m_bFinalStage=false;
