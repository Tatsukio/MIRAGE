		return;
	endproc;
	
	export proc void SetProcess(int p_iP)
		super.SetProcess(p_iP);
		if(p_iP==40 && !m_bParts)then
			m_bParts=true;
			var ^CMirageDeco pxParentObj=cast<CMirageDeco>(m_xDummies[0].GetObj());
			var CFourCC xLink="NONE";
			var vec3 vPos=pxParentObj^.GetPos();
			var vec3 vRot=pxParentObj^.GetRotation();
			pxParentObj^.SetVisible(false);
			var ^CMirageDeco pxGameObj=cast<CMirageDeco>(CSrvWrap.GetObjMgr()^.CreateObj("ninigi_pirate_ship_deco_1",GetOwner(),vPos,vRot));
			AddGroupedChildren(pxGameObj^.GetGuid());
			pxGameObj^.LinkAction(GetHandle(),xLink,{0.0,-11.36,0.01});
			pxGameObj^.SetDestructLevel(2);
			m_xDummies.Include(pxGameObj^.GetHandle());
			pxGameObj=cast<CMirageDeco>(CSrvWrap.GetObjMgr()^.CreateObj("ninigi_pirate_ship_deco_2",GetOwner(),vPos,vRot));
			AddGroupedChildren(pxGameObj^.GetGuid());
			pxGameObj^.LinkAction(GetHandle(),xLink,{0.0,0.0,0.0});
			pxGameObj^.SetDestructLevel(2);
			m_xDummies.Include(pxGameObj^.GetHandle());
			pxGameObj=cast<CMirageDeco>(CSrvWrap.GetObjMgr()^.CreateObj("ninigi_pirate_ship_deco_3",GetOwner(),vPos,vRot));
			AddGroupedChildren(pxGameObj^.GetGuid());
			pxGameObj^.LinkAction(GetHandle(),xLink,{0.0,12.6,0.0});
			pxGameObj^.SetDestructLevel(2);
			m_xDummies.Include(pxGameObj^.GetHandle());
			pxGameObj=cast<CMirageDeco>(CSrvWrap.GetObjMgr()^.CreateObj("ninigi_pirate_ship_deco_4",GetOwner(),vPos,vRot));
			AddGroupedChildren(pxGameObj^.GetGuid());
			pxGameObj^.LinkAction(GetHandle(),xLink,{0.0,0.0,0.0});
			pxGameObj^.SetDestructLevel(2);
			m_xDummies.Include(pxGameObj^.GetHandle());
			AddGroupedChildren(pxGameObj^.GetGuid());
			m_xDummies[0]=CObjHndl.Invalid();
			RemGroupedChildren(pxParentObj^.GetGuid());
			pxParentObj^.Delete();
		elseif(p_iP==60 && !m_bFirstStage)then
			m_bFirstStage=true;
			var int i, iC=m_xDummies.NumEntries();
			for(i=0)cond(i<iC)iter(i++)do
				if(m_xDummies[i].IsValid())then
					m_xDummies[i].GetObj()^.SetDestructLevel(1);
