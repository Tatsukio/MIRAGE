		pxArc^<<m_iPaladin2;
		pxArc^<<m_iPaladin3;
		pxArc^<<m_iPaladin4;
		pxArc^<<m_iPhantomIncrease;
		pxArc^<<m_iPaladinIncrease;
		pxArc^<<m_iPhantomGrowth;
		pxArc^<<m_fBounty;
		iC=m_asConquerors.NumEntries();
