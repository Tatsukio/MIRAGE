		if(!HasPersonalRegion())then
			var vec3 vE;vE.SetXYZ(DISRUPT_RADIUS, DISRUPT_RADIUS, 0.0f);
			CreatePersonalRegion("PsiDisruptor_"+GetName()+"_"+Random.GetInt().ToString(), vE, 010b);
		endif;
	endproc;
	
	export proc string GetReportCode()
		return "defence";
	endproc;
	
endclass;
	
