//		endfor;
		pxArc^<<m_bLeveling;
		pxArc^<<m_bReapingOver;
		pxArc^<<m_iSeer;
		pxArc^<<m_iDefenders;
		pxArc^<<m_iConquerors;
		pxArc^<<m_iConqueror1;
		pxArc^<<m_iConqueror2;
		pxArc^<<m_iConqueror3;
		pxArc^<<m_iConqueror4;
		pxArc^<<m_iPaladins;
