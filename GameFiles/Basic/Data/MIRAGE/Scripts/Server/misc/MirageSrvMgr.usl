	export proc bool IsCustomMap(string p_sLevelName)
		var bool bCustom = false;
		if(m_pxCustomMapSettingsDB!=null)then
			//KLog.LogSpam("ParaworldFan-ALERT", "MirageSrvMgr:CheckCustomMap root node != null");
			var ^CPropDB.CNode pxLevelsNode=m_pxCustomMapSettingsDB^.Get("Levels");
			if(pxLevelsNode!=null)then
				p_sLevelName.Replace(" ", "_");
				//KLog.LogSpam("ParaworldFan-ALERT", "MirageSrvMgr:CheckCustomMap Looking for "+p_sLevelName+" with replaced spaces ...");
				var ^CPropDB.CNode pxLevelNameNode = pxLevelsNode^.Get(p_sLevelName);
				if(pxLevelNameNode!=null)then
					bCustom = true;
				endif;
			endif;
		endif;
		return bCustom;
	endproc;
	
	export proc bool CheckCustomMap(string p_sLevelName, string p_sCategory)
		//KLog.LogSpam("ParaworldFan-ALERT", "MirageSrvMgr:CheckCustomMap("+p_sLevelName+", "+p_sCategory+") begin");
		var bool bCustom = false;
		if(m_pxCustomMapSettingsDB!=null)then
			//KLog.LogSpam("ParaworldFan-ALERT", "MirageSrvMgr:CheckCustomMap root node != null");
			var ^CPropDB.CNode pxLevelsNode=m_pxCustomMapSettingsDB^.Get("Levels");
			if(pxLevelsNode!=null)then
				p_sLevelName.Replace(" ", "_");
				//KLog.LogSpam("ParaworldFan-ALERT", "MirageSrvMgr:CheckCustomMap Looking for "+p_sLevelName+" with replaced spaces ...");
				var ^CPropDB.CNode pxLevelNameNode = pxLevelsNode^.Get(p_sLevelName);
				if(pxLevelNameNode!=null)then
					//KLog.LogSpam("ParaworldFan-ALERT", "MirageSrvMgr:CheckCustomMap LevelNameNode "+pxLevelNameNode^.Name()+" matched.");
					var ^CPropDB.CNode pxCategoriesNode = pxLevelNameNode^.Get("Custom");
					if(pxCategoriesNode!=null)then
						var ^CPropDB.CNode pxCustomCategoryNode = pxCategoriesNode^.FindNode(p_sCategory, false);
						if(pxCustomCategoryNode!=null)then
							//KLog.LogSpam("ParaworldFan-ALERT", "MirageSrvMgr:CheckCustomMap Custom Category "+p_sCategory+" found.");
							var string sValue = pxCustomCategoryNode^.Value();
							bCustom = (sValue=="" || sValue.ToInt()==1);
						endif;
					endif;
				endif;
			endif;
		endif;
		var string sCustom = "non-custom";
		if(bCustom)then sCustom = "custom"; endif;
		//KLog.LogSpam("ParaworldFan-ALERT", "MirageSrvMgr:CheckCustomMap Level "+p_sLevelName+" has "+sCustom+" "+p_sCategory);
		//KLog.LogSpam("ParaworldFanALERT", "MirageSrvMgr:CheckCustomMap("+p_sLevelName+", "+p_sCategory+") end");
		return bCustom;
	endproc;
	
