		m_bPaused=false;
		while(m_axNests.NumEntries()>0)do
			m_axNests.DeleteEntry(0);
		endwhile;
		while(m_axResources.NumEntries()>0)do
			m_axResources.DeleteEntry(0);
		endwhile;
		while(m_axIdiObjects.NumEntries()>0)do
