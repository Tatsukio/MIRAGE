	///RequestPestPatrol
	export proc bool RequestPestPatrol(vec3 p_vDeathPos)
		if((m_bEcoAssist)&&(!m_bDefAssist)&&(!m_bAtkAssist))then return false; endif;
		if(m_pxBrain==null)then return false; endif;
		if(m_pxSensor==null)then return false; endif;
