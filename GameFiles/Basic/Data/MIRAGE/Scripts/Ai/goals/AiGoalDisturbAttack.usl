				if(xEnemyShip[i].GetType()=="BLDG")then break; endif;	
			endfor;
		endif;
		asAttacks.AddEntry("none");
		asAttacks.AddEntry("none");
		asAttacks.AddEntry("none");
		asAttacks.AddEntry("none");
		asAttacks.AddEntry("none");
		if(m_sTribe=="Hu")then
			if((m_sBehavior=="D")||(m_sBehavior=="Dodo"))then
				if(m_sGametype!="Defender")then
					if(m_bMultimap&&(m_sLevelName!="Single 01"&&m_sLevelName!="Single 02"&&
