	var ^CAiGoalMinistryOfDefense m_pxMOD;
	var ^CAiMaps m_pxMaps;
	var ^CAiModuleDefenseDefault m_pxDM;
	var ^CAiModuleFightDefault m_pxFM;
	
	static var int		ms_iHelpTimerInit;
	var int						m_iHelpCallTimer;
	var int						m_iMapWidth;
	var int						m_iMapHeight;
